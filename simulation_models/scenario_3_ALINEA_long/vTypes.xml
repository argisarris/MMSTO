<?xml version="1.0" encoding="UTF-8"?>
<!-- SUMO Vehicle Type Definitions for A3 Highway Network -->
<!-- Base Vehicle Behavior - Unified for Highway and Ramp Traffic -->
<!-- 5 Aggressiveness Levels: aggr1 (Least) -> aggr5 (Most Aggressive) -->
<!-- Note: These base types apply to ALL vehicles regardless of route type -->

<additional xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://sumo.dlr.de/xsd/additional_file.xsd">

    <!-- ========================================= -->
    <!-- BASE VEHICLE BEHAVIOR                     -->
    <!-- Applied to all traffic (highway & ramps)  -->
    <!-- ========================================= -->

    <!-- Base Car - Aggressiveness Level 1 (Least Aggressive) -->
    <vType id="car_aggr1" vClass="passenger" length="5.0" minGap="0.1"
           speedFactor="0.80" speedDev="0.08" tau="10.0" maxSpeed="33.0"
           accel="0.5" decel="3.5"
           lcCooperative="4.5" lcStrategic="50.0" lcSpeedGain="7.0"
           lcPushy="0.00" lcSublane="2.0" lcAssertive="0.01"
           color="0.8,1,0.8">
        <param key="laneChangeModel" value="SL2015"/>
    </vType>

    <!-- Base Car - Aggressiveness Level 2 -->
    <vType id="car_aggr2" vClass="passenger" length="5.0" minGap="0.1"
           speedFactor="0.85" speedDev="0.09" tau="9.8" maxSpeed="33.75"
           accel="0.7" decel="4.0"
           lcCooperative="4.2" lcStrategic="40.0" lcSpeedGain="8.0"
           lcPushy="0.01" lcSublane="3.0" lcAssertive="0.05"
           color="0.6,1,0.6">
        <param key="laneChangeModel" value="SL2015"/>
    </vType>

