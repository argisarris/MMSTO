    <!-- ========================================= -->
    <!-- ENTRY RAMP VEHICLE TYPES                  -->
    <!-- ========================================= -->
    <!--

    Original design: Ramp vehicles had much more aggressive merging behavior with:
    - Lower tau values (0.4-2.0 vs 8.5-11.0)
    - Higher acceleration (2.5-5.0 vs 0.3-1.1)
    - Much higher lcPushy (3.0-6.3 vs 0.00-0.08)
    - Much higher lcAssertive (8.0-12.0 vs 0.01-0.20)
    - Much lower minGapLat (0.02-0.20 vs 9.0-12.0)
    - Near-zero lcCooperative (0.00-0.05 vs 3.5-5.0)
    
    Updated design: Ramp vehicles are 20% more aggressive in key merging parameters
    lcPushy and lcAssertive are 2x (double) the base values
    -->

  
    <!-- Ramp Car - Aggressiveness Level 1 (Least Aggressive) -->
    <vType id="ramp_car_aggr1" vClass="passenger" length="5.0" minGap="0.1"
           speedFactor="0.80" speedDev="0.08" tau="8.0" maxSpeed="33.0"
           accel="0.6" decel="3.5"
           lcCooperative="3.6" lcStrategic="50.0" lcSpeedGain="7.0"
           lcPushy="0.00" lcSublane="2.0" lcAssertive="0.02"
           color="0.8,1,0.8">
        <param key="laneChangeModel" value="SL2015"/>
    </vType>

    <!-- Ramp Car - Aggressiveness Level 2 -->
